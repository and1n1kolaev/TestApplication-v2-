{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "vue@2.6.10",
      "destination": "wwwroot/vue/",
      "files": [
        "vue.min.js"
      ]
    },
    {
      "library": "notify.js@3.0.0",
      "destination": "wwwroot/lib/notify.js/",
      "files": [
        "notify.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@aspnet/signalr@1.1.4",
      "destination": "wwwroot/lib/@aspnet/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    }
  ]
}